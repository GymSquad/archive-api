            },
            {
              "campus_id": "clhjj1v9k0002gbgnbfju6luj",
              "department_id": "clhjj1vgl000pgbgnuso925sb",
              "office_id": "clhjj1vra0043gbgnh8wnz32k"
            }
          ],
          "website_name": "\u5716\u66f8\u9928",
